<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_1" name="Процесс покупки билета" processRef="Process_1" />
  </bpmn:collaboration>
  <bpmn:process id="Process_1" isExecutable="false">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Buyer" name="Покупатель">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SelectEvent</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SelectPayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Link_PaymentStart</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_NoTickets</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReceiveUserEmail</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Success</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_System" name="Система">
        <bpmn:flowNodeRef>Task_CheckAvailability</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Availability</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Link_ToPayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ConfirmOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PaymentGateway</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_PaymentStatus</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CreateOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Split</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Join</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_PaymentFailed</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Seller" name="Продавец">
        <bpmn:flowNodeRef>Task_ReceiveSellerEmail</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_1" name="Старт">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_SelectEvent" name="Выбор мероприятия и нажатие &#34;Купить&#34;">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_CheckAvailability" name="Проверка наличия билетов">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_Availability" name="Билеты есть?">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_NoTickets</bpmn:outgoing>
      <bpmn:outgoing>Flow_YesTickets</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="End_NoTickets" name="Мест нет">
      <bpmn:incoming>Flow_NoTickets</bpmn:incoming>
      <bpmn:messageEventDefinition />
    </bpmn:endEvent>
    <bpmn:intermediateThrowEvent id="Link_ToPayment" name="К оплате">
      <bpmn:incoming>Flow_YesTickets</bpmn:incoming>
      <bpmn:linkEventDefinition name="PaymentProcess" />
    </bpmn:intermediateThrowEvent>
    <bpmn:intermediateCatchEvent id="Link_PaymentStart" name="К оплате">
      <bpmn:outgoing>Flow_ToPaymentMethod</bpmn:outgoing>
      <bpmn:linkEventDefinition name="PaymentProcess" />
    </bpmn:intermediateCatchEvent>
    <bpmn:userTask id="Task_SelectPayment" name="Выбор способа оплаты">
      <bpmn:incoming>Flow_ToPaymentMethod</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ConfirmOrder" name="Подтверждение заказа">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_PaymentGateway" name="Обработка платежа шлюзом">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_PaymentStatus" name="Успешно?">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_PayFail</bpmn:outgoing>
      <bpmn:outgoing>Flow_PaySuccess</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_CreateOrder" name="Создание заказа в БД">
      <bpmn:incoming>Flow_PaySuccess</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:parallelGateway id="Gateway_Split">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_ToUser</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToSeller</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:receiveTask id="Task_ReceiveUserEmail" name="Получить билет на Email">
      <bpmn:incoming>Flow_ToUser</bpmn:incoming>
      <bpmn:outgoing>Flow_UserDone</bpmn:outgoing>
    </bpmn:receiveTask>
    <bpmn:receiveTask id="Task_ReceiveSellerEmail" name="Получить уведомление о продаже">
      <bpmn:incoming>Flow_ToSeller</bpmn:incoming>
      <bpmn:outgoing>Flow_SellerDone</bpmn:outgoing>
    </bpmn:receiveTask>
    <bpmn:parallelGateway id="Gateway_Join">
      <bpmn:incoming>Flow_UserDone</bpmn:incoming>
      <bpmn:incoming>Flow_SellerDone</bpmn:incoming>
      <bpmn:outgoing>Flow_Final</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_SelectEvent" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_SelectEvent" targetRef="Task_CheckAvailability" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_CheckAvailability" targetRef="Gateway_Availability" />
    <bpmn:sequenceFlow id="Flow_NoTickets" name="Нет" sourceRef="Gateway_Availability" targetRef="End_NoTickets" />
    <bpmn:sequenceFlow id="Flow_YesTickets" name="Да" sourceRef="Gateway_Availability" targetRef="Link_ToPayment" />
    <bpmn:sequenceFlow id="Flow_ToPaymentMethod" sourceRef="Link_PaymentStart" targetRef="Task_SelectPayment" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_SelectPayment" targetRef="Task_ConfirmOrder" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_ConfirmOrder" targetRef="Task_PaymentGateway" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_PaymentGateway" targetRef="Gateway_PaymentStatus" />
    <bpmn:sequenceFlow id="Flow_PayFail" name="Нет" sourceRef="Gateway_PaymentStatus" targetRef="End_PaymentFailed" />
    <bpmn:sequenceFlow id="Flow_PaySuccess" name="Да" sourceRef="Gateway_PaymentStatus" targetRef="Task_CreateOrder" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_CreateOrder" targetRef="Gateway_Split" />
    <bpmn:sequenceFlow id="Flow_ToUser" sourceRef="Gateway_Split" targetRef="Task_ReceiveUserEmail" />
    <bpmn:sequenceFlow id="Flow_ToSeller" sourceRef="Gateway_Split" targetRef="Task_ReceiveSellerEmail" />
    <bpmn:sequenceFlow id="Flow_UserDone" sourceRef="Task_ReceiveUserEmail" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_SellerDone" sourceRef="Task_ReceiveSellerEmail" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_Final" sourceRef="Gateway_Join" targetRef="End_Success" />
    <bpmn:endEvent id="End_PaymentFailed" name="Ошибка оплаты">
      <bpmn:incoming>Flow_PayFail</bpmn:incoming>
      <bpmn:messageEventDefinition />
    </bpmn:endEvent>
    <bpmn:endEvent id="End_Success" name="Заказ завершен">
      <bpmn:incoming>Flow_Final</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
